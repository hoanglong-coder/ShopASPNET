@{ 
    var lst = (List<string>)ViewData["Slide"];

}


<div class="slider-area">
    <div class="slider-active-3 owl-carousel slider-hm8 owl-dot-style">
        @foreach (var item in lst)
        {
            <!-- Slider Single Item Start -->
            <div class="slider-height-6 d-flex align-items-start justify-content-start bg-img" style='background-image: url(..@item);'>
            </div>
            <!-- Slider Single Item End -->
        }
    </div>
</div>
